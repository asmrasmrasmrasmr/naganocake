<div class="container">
  <div class="row mt-5">
  <h4 style="background-color:#EEEEEE;color:dark;display:inline">ジャンル編集</h4>
  </div>
  <div class="row mt-5">
<%= form_with model: @genre,url: admin_genre_path(@genre.id),method: :patch do |f| %>
<%= f.label :name, "ジャンル名"%>
<%= f.text_field :name %>
<%= f.submit '変更を保存',class: 'btn btn-success' %>
<% end %>
</div>
</div>